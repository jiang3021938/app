{
    "title": "documents",
    "type": "object",
    "description": "Uploaded lease documents",
    "properties": {
        "id": {
            "type": "integer",
            "description": "Primary key, auto-increment"
        },
        "user_id": {
            "type": "string",
            "description": "User ID who uploaded the document"
        },
        "file_name": {
            "type": "string",
            "description": "Original file name"
        },
        "file_key": {
            "type": "string",
            "description": "Storage key for the file"
        },
        "file_size": {
            "type": "integer",
            "description": "File size in bytes"
        },
        "status": {
            "type": "string",
            "description": "Processing status: pending, processing, completed, failed"
        },
        "created_at": {
            "type": "string",
            "format": "date-time",
            "description": "Upload timestamp"
        },
        "updated_at": {
            "type": "string",
            "format": "date-time",
            "description": "Last update timestamp"
        }
    },
    "required": [
        "id",
        "user_id",
        "file_name",
        "file_key",
        "status"
    ]
}